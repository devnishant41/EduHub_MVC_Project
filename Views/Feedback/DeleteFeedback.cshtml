@model Test_EduHub.Models.DTO.FeedbackModel


<h3>Are you sure you want to delete this feedback?</h3>

<div>
    <hr />
    <dl class="dl-horizontal">

        <strong> Message :   </strong>  @Html.DisplayFor(model => model.Feedback)


    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color mt-3">
            <input type="submit" value="Delete" class="btn  btn-sm btn-danger" /> |
            @Html.ActionLink("Back to FeedBackList", "DetailedFeedback")
        </div>
    }
</div>